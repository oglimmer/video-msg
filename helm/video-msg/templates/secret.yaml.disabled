apiVersion: v1
kind: Secret
metadata:
  name: {{ include "video-msg.fullname" . }}-secret
  labels:
    {{- include "video-msg.labels" . | nindent 4 }}
type: Opaque
data:
  database.user: {{ .Values.database.external.user | b64enc | quote }}
  database.password: {{ .Values.database.external.password | b64enc | quote }}
